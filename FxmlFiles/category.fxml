            <padding>
                <Insets left="8.0" top="8.0" />
            </padding></VBox>
   </left>
   <center>
      <TableView fx:id="testTableview" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
        <columns>
          <TableColumn prefWidth="75.0" text="C1" />
          <TableColumn prefWidth="75.0" text="C2" />
        </columns>
      </TableView>
